	<insert id="inserUser" parameterType="com.example.demo.vo.UserVO">
		INSERT INTO users(username, password, firstName, lastName, age, salary)
		VALUES(#{username}, #{password}, #{firstName}, #{lastName}, #{age}, #{salary})
	</insert>

	

